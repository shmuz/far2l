<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE catalog PUBLIC "-//Cail Lomecb//DTD Colorer CATALOG take5//EN"
  "http://colorer.sf.net/2003/catalog.dtd"[
  
<!ENTITY hrc "hrc">
<!ENTITY hrd "hrd">
 
<!ENTITY catalog-console SYSTEM "hrd/catalog-console.xml">
<!ENTITY catalog-rgb SYSTEM "hrd/catalog-rgb.xml">
<!ENTITY catalog-text SYSTEM "hrd/catalog-text.xml">
<!ENTITY catalog-user SYSTEM "hrd/catalog-user.xml">

]>

<catalog xmlns="http://colorer.sf.net/2003/catalog"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://colorer.sf.net/2003/catalog
                             http://colorer.sf.net/2003/catalog.xsd">

  <hrc-sets log-location="">

    <location link="&hrc;/proto.hrc"/>
    <location link="&hrc;/auto"/>

  </hrc-sets>

  <hrd-sets>
    &catalog-console;

    &catalog-rgb;

    &catalog-text;

    &catalog-user;
  </hrd-sets>

</catalog>
